{
  "name": "@statx/react",
  "version": "1.0.27",
  "private": false,
  "description": "Extry tiny smart statx manager",
  "keywords": [
    "statx",
    "state",
    "store",
    "management",
    "lit-element",
    "lit",
    "app",
    "shared",
    "global",
    "observer",
    "observable"
  ],
  "homepage": "https://github.com/kaifaty/statx/packages/react",
  "bugs": {
    "url": "https://github.com/kaifaty/statx/issues"
  },
  "license": "MIT",
  "author": "kaifaty",
  "sideEffects": false,
  "exports": {
    "require": "./build/index.js",
    "types": "./build/index.d.ts",
    "default": "./build/index.modern.mjs"
  },
  "main": "./build/index.js",
  "module": "./build/index.module.js",
  "unpkg": "./build/index.umd.js",
  "types": "./build/index.d.ts",
  "files": [
    "/build",
    "/package.json",
    "/README.md",
    "/src"
  ],
  "scripts": {
    "build": "rimraf ./build/ && npm run build:mb && npm run build:types",
    "build:parcel": "parcel build",
    "build:mb": "microbundle",
    "build:types": "tsc",
    "dev": "parcel watch",
    "test": "tsx src/index.test.ts",
    "test:watch": "tsx watch src/index.test.ts"
  },
  "browserslist": [
    "last 4 chrome versions"
  ],
  "dependencies": {
    "@statx/core": "^1.0.27",
    "react": "^18.2.0"
  },
  "targets": {
    "build": {
      "context": "browser",
      "distDir": "./build/",
      "source": "./src/index.ts",
      "isLibrary": true,
      "optimize": true,
      "outputFormat": "esmodule"
    }
  },
  "gitHead": "c7246e5ac31c0ebca1a385e957164db3bacf7fd0"
}
